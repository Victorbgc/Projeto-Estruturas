typedef struct sDeque{
	int inicio, fim, quantidade;
	Baralho cartas[52];
} Deque;

Deque *criaDeque(){
	Deque *d;
	d = (Deque*)malloc(sizeof(Deque));
	
	if(d == NULL){
		printf("Erro na alocacao!\n");
	}
	
	d->inicio = 0;
	d->fim = 0;
	d->quantidade = 0;
	
	return 0;
}



int main(){
	Deque *d;
}
